    And Ich sehe die Uebersicht Stammdaten                     # StammdatenSuchen.ich_sehe_die_Uebersicht_Stammdaten()
    When Ich gebe im Suchfeld folgende Parameters '1' ,'1','1' # StammdatenSuchen.ich_gebe_im_Suchfeld_folgende_Parameters(String,String,String)
    And Ich klicke Suchen                                      # StammdatenSuchen.ich_klicke_Suchen()
